<?xml version='1.0' encoding='utf-8'?>
<widget id="com.asctbinf.novabrink" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>Novabrink</name>
    <description>
        Aplicativo para representantes comerciais.
    </description>
    <author email="alexandre@asctbinf.com" href="http://asctbinf.com">
        AS Informatica
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
</widget>
