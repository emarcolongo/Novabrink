<?xml version='1.0' encoding='utf-8'?>
<widget id="com.asctbinf.novabrink" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>Plastoy</name>
    <description>
        Aplicativo para representantes comerciais.
    </description>
    <author email="alexandre@asctbinf.com" href="http://asctbinf.com">
        AS Informatica
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
	<platform name="android">
        <icon density="ldpi" src="www/icon/android/drawable-ldpi-icon.png" />
        <icon density="mdpi" src="www/icon/android/drawable-mdpi-icon.png" />
        <icon density="hdpi" src="www/icon/android/drawable-hdpi-icon.png" />
        <icon density="xhdpi" src="www/icon/android/drawable-xhdpi-icon.png" />
        <icon density="xxhdpi" src="www/icon/android/drawable-xxhdpi-icon.png" />
        <icon density="xxxhdpi" src="www/icon/android/drawable-xxxhdpi-icon.png" />
	</platform>
</widget>
